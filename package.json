{
  "name": "remark-line-input",
  "version": "0.3.2",
  "description": "A remark plugin to parse line input syntax",
  "main": "app.js",
  "scripts": {
    "pretest": "xo",
    "test": "ava",
    "start": "node app.js"
  },
  "dependencies": {
    "md-attr-parser": "^1.0.0"
  },
  "author": "ache",
  "license": "MIT",
  "keywords": [
    "line",
    "input",
    "remark"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "dom5": "^3.0.0",
    "parse5": "^5.0.0",
    "rehype-raw": "^2.0.0",
    "rehype-stringify": "^3.0.0",
    "remark-parse": "^5.0.0",
    "remark-rehype": "^3.0.0",
    "unified": "^7.0.0",
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
