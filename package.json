{
  "name": "remark-line-input",
  "version": "0.6.0",
  "description": "A remark plugin to parse line input syntax",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "xo",
    "prepare": "del-cli dist && cross-env BABEL_ENV=production babel src --out-dir dist",
    "test": "ava"
  },
  "dependencies": {
    "md-attr-parser": "^1.2.0"
  },
  "author": "ache",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "line",
    "input",
    "remark"
  ],
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/plugin-proposal-object-rest-spread": "7.x",
    "@babel/preset-env": "7.x",
    "ava": "1.x",
    "cross-env": "5.x",
    "del-cli": "1.x",
    "dom5": "3.x",
    "parse5": "5.x",
    "rehype-raw": "4.x",
    "rehype-stringify": "5.x",
    "remark-parse": "6.x",
    "remark-rehype": "4.x",
    "unified": "7.x",
    "xo": "0.23.x"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
