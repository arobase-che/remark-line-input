{
  "name": "remark-line-input",
  "version": "0.1.0",
  "description": "A remark plugin to parse line input syntax.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "dependencies": {
    "unist-util-visit": "^1.1.3"
  },
  "author": "ache",
  "license": "MIT",
  "keywords": [
    "line",
    "input",
    "remark"
  ],
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo" : {
    "space" : true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }

  }
}
